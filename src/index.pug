extends pug/global
block content
  
  header
    //- h1 alani marcel—terapeuta kósmico
  
  ul.heart
    li.pulse
    li.pulse
    li.pulse
  
  section.hexagon-sincos.dev
    - for (var x = 0; x < 1; x++)
      section.layer
        ul
          - for (var y = 0; y < 6; y++)
            li
    
    //- always in the center
    section.center
      ul
        - for (var x = 0; x < 6; x++)
          li
  
  ul.social
    li #[a(href='javascript:void(0)' title='Facebook' target='_blank') Facebook]
    li #[a(href='javascript:void(0)' title='Instagram' target='_blank') Instagram]
